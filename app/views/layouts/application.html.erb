<!DOCTYPE html>
<html>
  <head>
    <title>WhimsyNotes</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= stylesheet_link_tag    'session_form', media: 'all' %>
    <%= stylesheet_link_tag    'splash', media: 'all' %>
    <%= stylesheet_link_tag    'sidebar', media: 'all' %>
    <%= stylesheet_link_tag    'index', media: 'all' %>


    <link rel="stylesheet" href="//cdn.quilljs.com/1.2.6/quill.snow.css">
    <script src="//cdn.quilljs.com/1.3.6/quill.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Montserrat|Ubuntu|Nunito|Raleway:200" rel="stylesheet">
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <script type="text/text/javascript">
      window.main_green = <%= image_url('main-green.png') %>
    </script>



    <%= yield %>
  </body>
</html>
